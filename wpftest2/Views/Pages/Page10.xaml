<Page x:Class="wpftest2.Views.Pages.Page10"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpftest2.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page10">

    <Grid Background="White">
        <Button Content="Назад" Height="50" Width="200" VerticalAlignment="Bottom" Click="Button_Click"/>
        <Menu Height="100" Width="200" Background="Aqua">
            <MenuItem Header="File" Background="White">
                <MenuItem Header="New project"></MenuItem>
                <MenuItem Header="Open project">
                    <MenuItem Header="WinForms"></MenuItem>
                    <MenuItem Header="Wpf"></MenuItem>
                </MenuItem>
                <Separator/>
                <MenuItem Header="Exit"></MenuItem>
            </MenuItem>
            <MenuItem Header="Edit" Background="Blue"></MenuItem>
            <MenuItem Header="View" Background="Red"></MenuItem>
        </Menu>

    </Grid>
</Page>

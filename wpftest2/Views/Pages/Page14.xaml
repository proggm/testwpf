<Page x:Class="wpftest2.Views.Pages.Page14"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpftest2.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page14">

    <Grid Background="White">
        <Button Content="Назад" Height="50" Width="200" VerticalAlignment="Bottom" Click="Button_Click"/>
        <Image Grid.Column="0" Margin="0,0,2,50">
            <Image.Source>
                <FormatConvertedBitmap Source="/Resourse/иконка.jpg" DestinationFormat="gray32Float"/>
            </Image.Source>
        </Image>
        <StackPanel Grid.Column="1">
            <Image Source="/Resourse/ЙЙЙ.jpg"/>
            <Image Source="/Resourse/ККК.jpg"/>
            <Image Source="/Resourse/МММ.jpg"/>
        </StackPanel>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
    </Grid>
</Page>

<Page x:Class="wpftest2.Views.Pages.Page11"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpftest2.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page11">

    <Grid Background="White">
        <Button Content="Назад" Height="50" Width="200" VerticalAlignment="Bottom" Click="Button_Click"/>
        <DataGrid Width="300" Height="200" VerticalAlignment="Top" AutoGenerateColumns="True" GridLinesVisibility="All">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Title" Width="90"></DataGridTextColumn>
                <DataGridTextColumn Header="Company" Width="90"></DataGridTextColumn>
                <DataGridTextColumn Header="Price" Width="90"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
